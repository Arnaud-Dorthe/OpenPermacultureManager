<?xml version="1.0"?>
<procedures xmlns="http://www.ekylibre.org/XML/2013/procedures">
  <procedure name="generic_tillage" categories="soil_working" optional-actions="weeding, loosening">
    <parameters>
      <target name="cultivation" filter="(is plant or is land_parcel)"/>
      <doer name="driver" filter="is worker and can drive(equipment)" cardinality="*"/>
      <doer name="doer" filter="is worker" cardinality="*"/>
      <tool name="tractor" filter="is motorized_vehicle" cardinality="*"/>
      <tool name="soil_tool" filter="is trailed_equipment" cardinality="*"/>
    </parameters>
  </procedure>
</procedures>
