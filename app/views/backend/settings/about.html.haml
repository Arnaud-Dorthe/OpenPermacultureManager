  = link_to('https://ekylibre.com/about/', target: '_blank', class: :logo) do
    = image_tag('logo.svg')
  - for p in @properties
    .med-info
      %span.title= p[0]
      %span.value= p[1]

  .med-info
    %span.value= link_to(:consult_ekylibre_api.tl, 'https://ekylibre.com/api', target: '_blank', style: 'color:rgb(100, 148, 193)')
      
  = cobbles do |c|
    - c.cobble :datasources do
      = cobble_list :datasources

